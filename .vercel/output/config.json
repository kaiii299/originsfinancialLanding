{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/about-us\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/testimonials\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blogs\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/career\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/contact\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/our-team\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/products\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/services\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/testimonials\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}